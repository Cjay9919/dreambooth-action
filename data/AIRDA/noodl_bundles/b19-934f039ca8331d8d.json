\n        image.src = src;\r\n    }\r\n\r\n    // Event listeners for drawing on the canvas\r\n    canvas.addEventListener('mousedown', (e) => {\r\n        isDrawing = true;\r\n        startX = e.offsetX;\r\n        startY = e.offsetY;\r\n    });\r\n\r\n    canvas.addEventListener('mousemove', (e) => {\r\n        if (!isDrawing) return;\r\n        // Redraw the image to clear previous drawings\r\n        loadImage(Noodl.getProjectVariable('selectedImage'));\r\n        // Draw the current selection\r\n        ctx.strokeRect(startX, startY, e.offsetX - startX, e.offsetY - startY);\r\n    });\r\n\r\n    canvas.addEventListener('mouseup', () => {\r\n        isDrawing = false;\r\n    });\r\n\r\n    // Example of how to set the image, adjust according to how you receive the image path\r\n    Noodl.onProjectVariableChanged('selectedImage', function(value) {\r\n        loadImage(value);\r\n    });\r\n</script>\r\n"},"ports":[],"children":[]}]}],"connections":[],"ports":[],"roots":["5b9fe1d0-b7da-59cd-2d07-93d858dfbd0f"]},{"name":"/Progress Circle","nodes":[{"id":"7b785b84-9259-5692-3d40-9df4a44b4b7f","type":"Group","parameters":{"sizeMode":"contentSize"},"ports":[],"children":[{"id":"a9b7c05a-1688-b121-cb8d-089b508b251a","type":"Circle","parameters":{"size":80,"strokeWidth":4,"fillEnabled":false,"strokeEnabled":true,"strokeColor":"Grey - 400"},"ports":[],"children":[]},{"id":"352581ef-c2f5-b3c5-1f66-a19eecefaa26","type":"Circle","parameters":{"size":80,"strokeWidth":4,"position":"absolute","fillEnabled":false,"strokeColor":"Primary","strokeLineCap":"round","strokeEnabled":true,"endAngle":0},"ports":[],"children":[]},{"id":"7330a190-d84d-d6ef-83a8-6fb1cedfa10d","type":"Group","parameters":{"sizeMode":"contentSize","position":"absolute","alignX":"center","alignY":"center"},"ports":[],"children":[{"id":"ab432f68-0d13-9340-527d-17f79e82414b","type":"Text","parameters":{"textStyle":"Label Large","text":"99%","color":"Grey - 900","lineHeight":{"value":1,"unit":""}},"ports":[],"ch